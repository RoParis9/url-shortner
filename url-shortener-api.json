{
  "name": "URL Shortener API",
  "description": "Complete API collection for URL Shortener application",
  "base_url": "http://localhost:3000",
  "variables": {
    "access_token": "",
    "refresh_token": "",
    "url_id": "url123",
    "user_email": "user@example.com",
    "user_password": "password123"
  },
  "requests": [
    {
      "name": "Health Check",
      "method": "GET",
      "url": "{{base_url}}/health",
      "description": "Check if the API is running"
    },
    {
      "name": "Root Endpoint",
      "method": "GET",
      "url": "{{base_url}}/",
      "description": "Get API information and available endpoints"
    },
    {
      "name": "Register User",
      "method": "POST",
      "url": "{{base_url}}/api/v1/auth/register",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "email": "{{user_email}}",
        "password": "{{user_password}}"
      },
      "description": "Register a new user account"
    },
    {
      "name": "Login User",
      "method": "POST",
      "url": "{{base_url}}/api/v1/auth/login",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "email": "{{user_email}}",
        "password": "{{user_password}}"
      },
      "description": "Login with email and password"
    },
    {
      "name": "Refresh Token",
      "method": "POST",
      "url": "{{base_url}}/api/v1/auth/refresh",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "refreshToken": "{{refresh_token}}"
      },
      "description": "Refresh access token using refresh token"
    },
    {
      "name": "Logout User",
      "method": "POST",
      "url": "{{base_url}}/api/v1/auth/logout",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "refreshToken": "{{refresh_token}}"
      },
      "description": "Logout and invalidate refresh token"
    },
    {
      "name": "Get User Profile",
      "method": "GET",
      "url": "{{base_url}}/api/v1/users/profile",
      "headers": {
        "Authorization": "Bearer {{access_token}}"
      },
      "description": "Get current user profile information"
    },
    {
      "name": "Update User Profile",
      "method": "PUT",
      "url": "{{base_url}}/api/v1/users/profile",
      "headers": {
        "Authorization": "Bearer {{access_token}}",
        "Content-Type": "application/json"
      },
      "body": {
        "email": "newemail@example.com",
        "currentPassword": "{{user_password}}",
        "newPassword": "newpassword123"
      },
      "description": "Update user profile information"
    },
    {
      "name": "Delete User Account",
      "method": "DELETE",
      "url": "{{base_url}}/api/v1/users/account",
      "headers": {
        "Authorization": "Bearer {{access_token}}",
        "Content-Type": "application/json"
      },
      "body": {
        "password": "{{user_password}}"
      },
      "description": "Delete user account permanently"
    },
    {
      "name": "Create URL (Authenticated)",
      "method": "POST",
      "url": "{{base_url}}/api/v1/urls",
      "headers": {
        "Authorization": "Bearer {{access_token}}",
        "Content-Type": "application/json"
      },
      "body": {
        "originalUrl": "https://example.com/very-long-url",
        "customShortCode": "custom123"
      },
      "description": "Create a new shortened URL (requires authentication)"
    },
    {
      "name": "Create URL (Public)",
      "method": "POST",
      "url": "{{base_url}}/api/v1/urls/public",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "originalUrl": "https://example.com/very-long-url",
        "customShortCode": "custom123"
      },
      "description": "Create a new shortened URL (public endpoint)"
    },
    {
      "name": "Get User URLs",
      "method": "GET",
      "url": "{{base_url}}/api/v1/urls?page=1&limit=10&sortBy=createdAt&sortOrder=desc",
      "headers": {
        "Authorization": "Bearer {{access_token}}"
      },
      "description": "Get paginated list of user's URLs"
    },
    {
      "name": "Update URL",
      "method": "PUT",
      "url": "{{base_url}}/api/v1/urls/{{url_id}}",
      "headers": {
        "Authorization": "Bearer {{access_token}}",
        "Content-Type": "application/json"
      },
      "body": {
        "originalUrl": "https://newexample.com/updated-url",
        "customShortCode": "newcustom123"
      },
      "description": "Update an existing URL"
    },
    {
      "name": "Delete URL",
      "method": "DELETE",
      "url": "{{base_url}}/api/v1/urls/{{url_id}}",
      "headers": {
        "Authorization": "Bearer {{access_token}}"
      },
      "description": "Delete a URL"
    },
    {
      "name": "Bulk Create URLs",
      "method": "POST",
      "url": "{{base_url}}/api/v1/urls/bulk",
      "headers": {
        "Authorization": "Bearer {{access_token}}",
        "Content-Type": "application/json"
      },
      "body": {
        "urls": [
          {
            "originalUrl": "https://example1.com",
            "customShortCode": "custom1"
          },
          {
            "originalUrl": "https://example2.com",
            "customShortCode": "custom2"
          }
        ]
      },
      "description": "Create multiple URLs in a single request"
    },
    {
      "name": "Redirect to Original URL",
      "method": "GET",
      "url": "{{base_url}}/custom123",
      "description": "Redirect to the original URL using short code"
    },
    {
      "name": "Get URL Analytics",
      "method": "GET",
      "url": "{{base_url}}/api/v1/analytics/urls/{{url_id}}?startDate=2024-01-01&endDate=2024-01-31&groupBy=day",
      "headers": {
        "Authorization": "Bearer {{access_token}}"
      },
      "description": "Get analytics data for a specific URL"
    },
    {
      "name": "Get Dashboard Stats",
      "method": "GET",
      "url": "{{base_url}}/api/v1/analytics/dashboard",
      "headers": {
        "Authorization": "Bearer {{access_token}}"
      },
      "description": "Get overall dashboard statistics"
    },
    {
      "name": "Swagger UI",
      "method": "GET",
      "url": "{{base_url}}/api-docs",
      "description": "Access Swagger UI documentation"
    },
    {
      "name": "Scalar Documentation",
      "method": "GET",
      "url": "{{base_url}}/docs",
      "description": "Access Scalar documentation"
    },
    {
      "name": "OpenAPI JSON",
      "method": "GET",
      "url": "{{base_url}}/api-docs/swagger.json",
      "description": "Get OpenAPI specification in JSON format"
    }
  ]
} 